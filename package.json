{
  "name": "text-club",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "dev:watch": "concurrently \"npm run dev\" \"nodemon --watch prisma/schema.prisma --ext prisma --exec 'npm run prisma:regen'\"",
    "prisma:regen": "prisma generate && npx kill-port 3000 || true",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@azure/msal-node": "^3.7.3",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/client": "^6.15.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "csv-parse": "^6.1.0",
    "isomorphic-fetch": "^3.0.0",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
